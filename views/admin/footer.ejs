    <script>
        // Sidebar Toggle
        document.addEventListener('DOMContentLoaded', function() {
            const sidebar = document.getElementById('sidebar');
            const mainContent = document.getElementById('main-content');
            const sidebarToggle = document.getElementById('sidebarToggle');
            const mobileSidebarToggle = document.getElementById('mobileSidebarToggle');
            const sidebarClose = document.getElementById('sidebarClose');
            const sidebarOverlay = document.getElementById('sidebarOverlay');
            
            // Function to check window width and set initial state
            function checkWidth() {
                if (window.innerWidth < 992) {
                    sidebar.classList.remove('collapsed');
                    sidebar.classList.remove('show');
                    mainContent.classList.remove('expanded');
                    sidebarOverlay.classList.remove('show');
                } else {
                    // Default to expanded sidebar on desktop
                    sidebar.classList.remove('collapsed');
                    mainContent.classList.remove('expanded');
                }
            }
            
            // Set initial state
            checkWidth();
            
            // Toggle sidebar on desktop button click
            sidebarToggle.addEventListener('click', function() {
                if (window.innerWidth >= 992) {
                    sidebar.classList.toggle('collapsed');
                    mainContent.classList.toggle('expanded');
                } else {
                    sidebar.classList.add('show');
                    sidebarOverlay.classList.add('show');
                }
            });
            
            // Toggle sidebar on mobile button click
            mobileSidebarToggle.addEventListener('click', function() {
                sidebar.classList.add('show');
                sidebarOverlay.classList.add('show');
            });
            
            // Close sidebar on X button click
            sidebarClose.addEventListener('click', function() {
                sidebar.classList.remove('show');
                sidebarOverlay.classList.remove('show');
            });
            
            // Close sidebar when clicking overlay
            sidebarOverlay.addEventListener('click', function() {
                sidebar.classList.remove('show');
                sidebarOverlay.classList.remove('show');
            });
            
            // Handle window resize
            window.addEventListener('resize', checkWidth);
            
            // Handle dropdown menus in collapsed sidebar
            const dropdownItems = document.querySelectorAll('.nav-item');
            dropdownItems.forEach(item => {
                item.addEventListener('mouseenter', function() {
                    if (sidebar.classList.contains('collapsed') && window.innerWidth >= 992) {
                        const submenu = this.querySelector('.submenu');
                        if (submenu) {
                            submenu.classList.add('show');
                        }
                    }
                });
                
                item.addEventListener('mouseleave', function() {
                    if (sidebar.classList.contains('collapsed') && window.innerWidth >= 992) {
                        const submenu = this.querySelector('.submenu');
                        if (submenu) {
                            submenu.classList.remove('show');
                        }
                    }
                });
            });
            
            // Initialize Chart
            const ctx = document.getElementById('waterQualityChart').getContext('2d');
            const waterQualityChart = new Chart(ctx, {
                type: 'line',
                data: {
                    labels: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'],
                    datasets: [
                        {
                            label: 'TDS Level',
                            data: [120, 135, 125, 130, 110, 105, 115],
                            borderColor: '#0891B2',
                            backgroundColor: 'rgba(8, 145, 178, 0.1)',
                            tension: 0.4,
                            fill: true
                        },
                        {
                            label: 'pH Level',
                            data: [7.2, 7.1, 7.3, 7.4, 7.2, 7.0, 7.1],
                            borderColor: '#10B981',
                            backgroundColor: 'rgba(16, 185, 129, 0.1)',
                            tension: 0.4,
                            fill: true
                        },
                        {
                            label: 'Chlorine',
                            data: [0.5, 0.6, 0.4, 0.5, 0.7, 0.6, 0.5],
                            borderColor: '#F59E0B',
                            backgroundColor: 'rgba(245, 158, 11, 0.1)',
                            tension: 0.4,
                            fill: true
                        }
                    ]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            position: 'top',
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: false
                        }
                    }
                }
            });
            
            // Handle Bootstrap dropdowns in mobile view
            if (window.innerWidth < 768) {
                const dropdownMenus = document.querySelectorAll('.dropdown-menu');
                dropdownMenus.forEach(menu => {
                    menu.classList.add('dropdown-menu-end');
                });
            }
            
            // Remove blue highlight on click for mobile devices
            document.addEventListener('click', function(e) {
                if (e.target.tagName === 'A' || e.target.tagName === 'BUTTON') {
                    e.target.blur();
                }
            });
        });
    </script>
